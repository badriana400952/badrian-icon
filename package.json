{
  "name": "badrian-icon",
  "version": "3.0.8",
  "private": false,
  "description": "Koleksi ikon SVG buatan Badrian sebagai React components.",
  "repository": "https://github.com/badrian/badrian-icon",
  "license": "MIT",
  "homepage": "https://portfolio-badriana.vercel.app/components/Introduction",
  "workspaces": [
    "packages/*"
  ],
  "exports": {
    "./food": {
      "import": "./packages/icon-food/dist/index.esm.js",
      "require": "./packages/icon-food/dist/index.cjs.js",
      "types": "./packages/icon-food/dist/index.d.ts"
    },
    "./bendera": {
      "import": "./packages/icon-bendera/dist/index.esm.js",
      "require": "./packages/icon-bendera/dist/index.cjs.js",
      "types": "./packages/icon-bendera/dist/index.d.ts"
    },
    "./kalm": {
      "import": "./packages/icon-kalm/dist/index.esm.js",
      "require": "./packages/icon-kalm/dist/index.cjs.js",
      "types": "./packages/icon-kalm/dist/index.d.ts"
    },
    "./instagram": {
      "import": "./packages/icon-instagram/dist/index.esm.js",
      "require": "./packages/icon-instagram/dist/index.cjs.js",
      "types": "./packages/icon-instagram/dist/index.d.ts"
    },
    "./kryston": {
      "import": "./packages/icon-kryston/dist/index.esm.js",
      "require": "./packages/icon-kryston/dist/index.cjs.js",
      "types": "./packages/icon-kryston/dist/index.d.ts"
    },
    "./leonid": {
      "import": "./packages/icon-leonid/dist/index.esm.js",
      "require": "./packages/icon-leonid/dist/index.cjs.js",
      "types": "./packages/icon-leonid/dist/index.d.ts"
    },
    "./logos": {
      "import": "./packages/icon-logos/dist/index.esm.js",
      "require": "./packages/icon-logos/dist/index.cjs.js",
      "types": "./packages/icon-logos/dist/index.d.ts"
    },
    "./rounak": {
      "import": "./packages/icon-Rounak/dist/index.esm.js",
      "require": "./packages/icon-Rounak/dist/index.cjs.js",
      "types": "./packages/icon-Rounak/dist/index.d.ts"
    }
  },
  "files": [
    "packages/icon-food/dist",
    "packages/icon-bendera/dist",
    "packages/icon-kalm/dist",
    "packages/icon-instagram/dist",
    "packages/icon-kryston/dist",
    "packages/icon-leonid/dist",
    "packages/icon-logos/dist",
    "packages/icon-Rounak/dist"
  ],
  "scripts": {
    "build:all": "npm --workspaces run build",
    "tai": "npm version patch && npm publish --access public"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "rollup": "^4.21.0",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.4.0"
  }
}
